{
	"name": "imperial-node",
	"version": "1.4.2",
	"description": "Imperial API wrapper for Node.js",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"build": "rimraf lib && tsc",
		"lint": "eslint src/**/*.ts --fix",
		"format": "prettier --write 'src/**/*.ts'",
		"test": "jest",
		"lazy": "yarn format && yarn lint && yarn build && yarn test",
		"ci": "rm -rf node_modules && yarn install --frozen-lockfile"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imperialbin/imperial-node.git"
	},
	"keywords": [
		"imperial",
		"imperialb.in"
	],
	"author": "pxseu <contact.pxseu@gmail.com>",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/imperialbin/imperial-node/issues"
	},
	"homepage": "https://github.com/imperialbin/imperial-node#readme",
	"dependencies": {},
	"devDependencies": {
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.20",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"dotenv": "^8.2.0",
		"eslint": "^7.17.0",
		"eslint-config-prettier": "^7.1.0",
		"jest": "^26.6.3",
		"nock": "^13.0.11",
		"prettier": "^2.2.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.5.1",
		"typescript": "^4.1.5"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"testMatch": [
			"**/test/**/*.+(ts|js)"
		],
		"transform": {
			"^.+\\.ts$": "ts-jest"
		}
	}
}
